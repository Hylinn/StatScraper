<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="io.github.hylinn.hibernate.statistics.SeasonsEntity" table="seasons" schema="" catalog="statistics">
        <composite-id mapped="true" class="io.github.hylinn.hibernate.statistics.SeasonsEntityPK">
            <key-property name="year" column="year"/>
            <key-property name="playSeason" column="play_season"/>
        </composite-id>
        <property name="id" column="id"/>
    </class>
</hibernate-mapping>