<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="io.github.hylinn.hibernate.statistics">

    <class name="LeagueDivisionsEntity" table="league_divisions" schema="" catalog="statistics">
        <composite-id mapped="true" class="LeagueDivisionsEntityPK">
            <key-property name="seasonLeague" column="season_league"/>
            <key-property name="division" column="division"/>
        </composite-id>
        <property name="id" column="id"/>
    </class>
</hibernate-mapping>