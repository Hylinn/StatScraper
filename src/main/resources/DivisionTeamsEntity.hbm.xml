<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="io.github.hylinn.hibernate.statistics">

    <class name="DivisionTeamsEntity" table="division_teams" schema="" catalog="statistics">
        <composite-id mapped="true" class="DivisionTeamsEntityPK">
            <key-property name="leagueDivision" column="league_division"/>
            <key-property name="team" column="team"/>
        </composite-id>
        <property name="id" column="id"/>
    </class>
</hibernate-mapping>