<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="io.github.hylinn.hibernate.statistics">

    <class name="SeasonLeaguesEntity" table="season_leagues" schema=""
           catalog="statistics">
        <composite-id mapped="true" class="SeasonLeaguesEntityPK">
            <key-property name="league" column="league"/>
            <key-property name="season" column="season"/>
        </composite-id>
        <property name="id" column="id"/>
    </class>
</hibernate-mapping>